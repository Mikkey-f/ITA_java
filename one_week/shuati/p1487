https://www.luogu.com.cn/problem/P1478

#include<bits/stdc++.h> 
using namespace std;
int n,s;
struct ss{
	int x;
	int y;
}t[5010];
bool cmp(ss a,ss b){
	return a.y<b.y;
}
int main(){
	cin>>n>>s;
	int a,b;
	cin>>a>>b;
	for(int i=0;i<n;i++){
	cin>>t[i].x>>t[i].y;
}
	sort(t,t+n,cmp);
	int sum=0;
	for(int i=0;i<n;i++){
		if(s>=t[i].y&&(a+b)>=t[i].x){
			s-=t[i].y;
			sum++;
		}
		if(s<0)break;
	}
	cout<<sum;
	return 0;
}
