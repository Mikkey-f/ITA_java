https://www.luogu.com.cn/problem/P2440

#include<bits/stdc++.h> 
using namespace std;
long long int n,k,a[100010];
int ss(int b){
	int c=0;
	for(int i=1;i<=n;i++){
		if(a[i]>=b){
			c+=a[i]/b;
			if(c>=k)return 1;
		}
	}
	return 0;
}
int main(){
	cin>>n>>k;
	for(int i=1;i<=n;i++)cin>>a[i];
	long long int l=0,r=100000001,m=0;
	while(l+1<r){
		m=l+(r-l)/2;
		if(ss(m)==1)l=m;
		else r=m;
	}
	cout<<l;
	return 0;
} 
