https://www.luogu.com.cn/problem/P1152

#include<bits/stdc++.h> 
using namespace std;
int n,a[1005],c[1005];
map<int,int>b;//因为要记录1~n-1是否都出现了 
int main(){
	cin>>n;
	cin>>a[1];
	for(int i=2;i<=n;i++){
		cin>>a[i];
		//c[i-1]=a[i]-a[i-1];//差分数组 
		b[abs(a[i]-a[i-1])]++;
	}
	for(int i=1;i<n;i++){
		if(b[i]==0){
			cout<<"Not jolly";
			return 0;
		}
	}
	cout<<"Jolly";
	return 0;
} 
